    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@gramio/callback-data": "^0.0.3",
    "@gramio/contexts": "^0.0.20",
    "@gramio/files": "^0.0.11",
    "@gramio/format": "^0.1.3",
    "@gramio/keyboards": "^0.3.3",
    "@gramio/types": "^7.7.1",
    "debug": "^4.3.5",
    "inspectable": "^3.0.2",
    "middleware-io": "^2.8.1",
