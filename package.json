{
  "name": "gramio",
  "type": "commonjs",
  "version": "0.0.46",
  "description": "Powerful Telegram Bot API framework",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "telegram",
    "telegram-bot",
    "telegram-bot-api",
    "bot",
    "framework",
    "types",
    "client",
    "webhook",
    "long-polling"
